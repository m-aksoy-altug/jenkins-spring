<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC "-//Puppy Crawl//DTD Suppressions 1.1//EN"
  "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">
<suppressions>
    <!-- Suppress all checks for files matching the pattern "com.jenkins.*" -->
    <suppress files="com.jenkins.*" checks="."/>
    
    <!-- Suppress all checks for all files (not recommended unless necessary) -->
    <suppress files=".*" checks="."/>
</suppressions>